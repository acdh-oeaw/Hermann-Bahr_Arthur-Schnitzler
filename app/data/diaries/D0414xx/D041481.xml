   <text type="diaryDay">
      <body>
         <div type="writingSession" n="1">
            <p>
               <date when="19170200" n="01"/>
               <hi rend="underline" n="1">Visitkarten</hi>
            </p>
            <p>Früh <orgName key="pmb29788">Deutsche Paßstelle</orgName>
            </p>
            <p>Dienstag Vormittag: <persName key="pmb3727">Eberle</persName>
               <space unit="chars" quantity="1"/>
               <placeName key="pmb371">Döbling – Nußwaldgasse 12<hi rend="superscript">I</hi>
               </placeName> (<placeName key="pmb381">Carmeliten</placeName>
               <space unit="chars" quantity="1"/>
               <persName key="pmb28262">Willibrord</persName>)</p>
            <p>Nachmittag Frau <persName key="pmb27790">Benedikt</persName>
               <space unit="chars" quantity="1"/>
               <placeName key="pmb382">Wohllebengasse</placeName>
            </p>
            <p>Mittwoch Mittag <persName key="pmb10863">Richard Beerhofmann</persName>
               <space unit="chars" quantity="1"/>
               <placeName key="pmb383">Hasenauerstraße 59</placeName>
            </p>
            <p>von ihm zu</p>
            <p>
               <persName key="pmb3731">Kralik</persName>
               <space unit="chars" quantity="1"/>
               <placeName key="pmb383">Hasenauerstr. 20</placeName>
               <space unit="chars" quantity="1"/>
               <placeName key="pmb384">Karl Ludwigpl. 3</placeName>
            </p>
            <p>u. eventuell</p>
            <p>
               <persName key="pmb2121">Schnitzler</persName>
               <space unit="chars" quantity="1"/>
               <placeName key="pmb150">Sternwartestr. 71</placeName>
            </p>
            <p>
               <persName key="pmb2167">Salten</persName>?</p>
            <p>Vielleicht Mittwoch Vormittag <persName key="pmb28261">P. Szabo</persName>
               <space unit="chars" quantity="1"/>
               <placeName key="pmb385">Postgasse 4</placeName>
            </p>
            <p>20. <del rend="strikethrough">
                  <unclear>Mont</unclear>
               </del> Dienstag ganz frei für <persName key="pmb12481">Redlich</persName>.<pb/>
            </p>
            <p>
               <persName key="pmb28262">Verkade</persName>
               <space unit="chars" quantity="1"/>
               <placeName key="pmb327">
                  <settlement>Riedlingen/Donau Würtemberg</settlement>
               </placeName>
            </p>
            <p>Frau <persName key="pmb27826">Dora Breisach</persName> (<persName key="pmb18084">Irene Hellman</persName>s Freundin)</p>
            <p>
               <persName key="pmb2202">Wilhelm Singer</persName>, <persName key="pmb16958">Adolf Gelber</persName>
            </p>
            <p>
               <persName key="pmb10875">Benedikt</persName>
               <space unit="chars" quantity="1"/>
               <add place="inline">
                  <placeName key="pmb382">Wollebeng 6</placeName>
               </add>, <persName key="pmb3021">Sternberg</persName>
            </p>
            <p>
               <persName key="pmb28625">Baron Fuchs</persName>
               <space unit="chars" quantity="1"/>
               <del rend="strikethrough">
                  <placeName key="pmb786">Lugeck 6</placeName>
               </del> | <placeName key="pmb444">Jordang 9</placeName>
            </p>
            <p>
               <persName key="pmb3280">Kolo Moser</persName>
            </p>
            <p>
               <persName key="pmb12954">Jaroslav Thun</persName>
               <space unit="chars" quantity="1"/>
               <placeName key="pmb498">Am Hof 13.</placeName>
            </p>
            <p>Generaladjutant Generalmajor Pz <persName key="pmb4291">Lobkowitz</persName>
            </p>
            <p>
               <hi rend="underline" n="1">
                  <persName key="pmb2686">Conrad Hohenlohe</persName>
               </hi> (<orgName key="pmb29760">Kaiser Karl Gesellschaft</orgName>)</p>
            <p>
               <persName key="pmb11646">Hugo Heller</persName>
            </p>
            <p>Fürstin <persName key="pmb12231">Metternich</persName>
               <space unit="chars" quantity="1"/>
               <placeName key="pmb528">Fasangasse 34</placeName>
            </p>
            <p>
               <orgName key="pmb29320">Journal</orgName>, <persName key="pmb3579">Jacobson</persName>, Abds gegen 7</p>
            <p>
               <hi rend="underline" n="1">
                  <persName key="pmb10765">Poldi Andrian</persName>
               </hi>
            </p>
            <p>Gräfin <persName key="pmb26046">Thun</persName>
               <space unit="chars" quantity="1"/>
               <placeName key="pmb529">Hoher Markt</placeName>
               <space unit="chars" quantity="1"/>
               <placeName key="pmb787">Palais Sina</placeName>
            </p>
            <p>
               <persName key="pmb3916">Ludwig Klinenberger</persName>
               <space unit="chars" quantity="1"/>
               <orgName key="pmb29351">N. Fr. Pr.</orgName> und Generalstabsoberst 
            <persName key="pmb28336">Wilhelm Eisner-Bubna</persName>
            </p>
         </div>
      </body>
