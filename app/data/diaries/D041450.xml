         <div type="writingSession" n="1"> 
            <p> 
               <date when="19030102" n="01"/> 
               <placeName key="pmb336">Frick</placeName>: <persName key="pmb20315"> 
                  <workName key="pmb31109">Leonardo</workName> 
               </persName> – <persName key="pmb3542">Knackfuß</persName> 
            </p> 
            <p> 
               <persName key="pmb11632">Heymann</persName>: <workName key="pmb30105">Kritik der Kritik</workName> 
            </p> 
            <p>Ob <hi rend="underline" n="1"> 
                  <persName key="pmb2665">Bell</persName> 
               </hi> oder <workName key="pmb31111">Masters in Art <persName key="pmb20315">Leonardo</persName> 
               </workName> 
            </p> 
            <p> 
               <persName key="pmb3539">Zillner</persName> »<workName key="pmb31107">Gesch der Stadt <placeName key="pmb30"> 
                     <settlement>Salzburg</settlement> 
                  </placeName> 
               </workName>« 1890 u 95 </p> 
            <p> 
               <table cols="2" rows="5"> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1">Morgen: </cell> 
                     <cell role="data" rows="1" cols="1"> 
                        <persName key="pmb2202">Singer</persName> oder <persName key="pmb12796">Skrein</persName> 
                     </cell> 
                  </row> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1"/> 
                     <cell role="data" rows="1" cols="1"> 
                        <persName key="pmb2121">Schnitzler</persName> 
                     </cell> 
                  </row> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1"/> 
                     <cell role="data" rows="1" cols="1">Redactionstrinkgelder</cell> 
                  </row> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1"/> 
                     <cell role="data" rows="1" cols="1">Briefpapier</cell> 
                  </row> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1"/> 
                     <cell role="data" rows="1" cols="1">Leiter neben <placeName key="pmb337">Hotel de France</placeName> (2 Meter 70, da Kasten 2 M 30 ist)</cell> 
                  </row> 
               </table> 
            </p> 
            <p> 
               <table cols="3" rows="16"> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1">Einnahmen</cell> 
                     <cell role="data" rows="1" cols="1">Von oder für</cell> 
                     <cell role="data" rows="1" cols="1"> 
                        <anchor type="textConst" xml:id="TD450-1">Ausgaben</anchor> 
                     </cell> 
                  </row> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1"/> 
                     <cell role="data" rows="1" cols="1">Nach Bezalg von 500 Kr an <persName key="pmb13682">Rosa</persName> bleiben mir</cell> 
                     <cell role="data" rows="1" cols="1"/> 
                  </row> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1">600 Kr.</cell> 
                     <cell role="data" rows="1" cols="1"/> 
                     <cell role="data" rows="1" cols="1"/> 
                  </row> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1"/> 
                     <cell role="data" rows="1" cols="1"> 
                        <persName key="pmb27721">Brieftr</persName> 
                     </cell> 
                     <cell role="data" rows="1" cols="1">10 Kr</cell> 
                  </row> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1"/> 
                     <cell role="data" rows="1" cols="1"> 
                        <persName key="pmb27722">Gemeindediener</persName> 
                     </cell> 
                     <cell role="data" rows="1" cols="1">2 Kr</cell> 
                  </row> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1"/> 
                     <cell role="data" rows="1" cols="1">für Hefte</cell> 
                     <cell role="data" rows="1" cols="1">4 Kr</cell> 
                  </row> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1"/> 
                     <cell role="data" rows="1" cols="1">Commission</cell> 
                     <cell role="data" rows="1" cols="1">1 Kr</cell> 
                  </row> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1"/> 
                     <cell role="data" rows="1" cols="1">Apotheke</cell> 
                     <cell role="data" rows="1" cols="1">2 68 h</cell> 
                  </row> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1"/> 
                     <cell role="data" rows="1" cols="1">Trinkgelder für Diener des <orgName key="pmb29414">N. W.</orgName> u. der <orgName key="pmb29421">Ö. V.</orgName> 
                     </cell> 
                     <cell role="data" rows="1" cols="1">30 Kr.</cell> 
                  </row> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1"/> 
                     <cell role="data" rows="1" cols="1"> 
                        <persName key="pmb28184">Ferdinand</persName> Comission</cell> 
                     <cell role="data" rows="1" cols="1">2</cell> 
                  </row> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1"/> 
                     <cell role="data" rows="1" cols="1">Wagen</cell> 
                     <cell role="data" rows="1" cols="1">3 30</cell> 
                  </row> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1"/> 
                     <cell role="data" rows="1" cols="1"> 
                        <persName key="pmb27721">Briefträger</persName> 
                     </cell> 
                     <cell role="data" rows="1" cols="1">3</cell> 
                  </row> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1"/> 
                     <cell role="data" rows="1" cols="1">Feuerwehrball</cell> 
                     <cell role="data" rows="1" cols="1">4</cell> 
                  </row> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1">39 Kr</cell> 
                     <cell role="data" rows="1" cols="1">vom <orgName key="pmb29655">Rubinverlag</orgName> am 4/1 erhalten</cell> 
                     <cell role="data" rows="1" cols="1"/> 
                  </row> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1"/> 
                     <cell role="data" rows="1" cols="1"/> 
                     <cell role="data" rows="1" cols="1"> 
                        <hi rend="overline">61 98</hi> 
                     </cell> 
                  </row> 
                  <row role="data" rows="1" cols="1"> 
                     <cell role="data" rows="1" cols="1"/> 
                     <cell role="data" rows="1" cols="1">An <persName key="pmb28076">Ludwig</persName> noch am 31. Dez. bezalt</cell> 
                     <cell role="data" rows="1" cols="1">30 Kr</cell> 
                  </row> 
               </table> 
            </p> 
