   <text type="diaryDay">
      <body>
         <div type="writingSession" n="1">
            <p>
               <date when="19030102" n="01"/>
               <rs type="place" ref="pmb336">Frick</rs>: <rs type="person" ref="pmb20315">
                  <rs type="work" ref="pmb31109">Leonardo</rs>
               </rs> – <rs type="person" ref="pmb3542">Knackfuß</rs>
            </p>
            <p>
               <rs type="person" ref="pmb11632">Heymann</rs>: <rs type="work" ref="pmb30105">Kritik der Kritik</rs>
            </p>
            <p>Ob <hi rend="underline" n="1">
                  <rs type="person" ref="pmb2665">Bell</rs>
               </hi> oder <rs type="work" ref="pmb31111">Masters in Art <rs type="person" ref="pmb20315">Leonardo</rs>
               </rs>
            </p>
            <p>
               <rs type="person" ref="pmb3539">Zillner</rs> »<rs type="work" ref="pmb31107">Gesch der Stadt <rs type="place" ref="pmb30">Salzburg</rs>
               </rs>« 1890 u 95 </p>
            <p>
               <table cols="2" rows="5">
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1">Morgen: </cell>
                     <cell role="data" rows="1" cols="1">
                        <rs type="person" ref="pmb2202">Singer</rs> oder <rs type="person" ref="pmb12796">Skrein</rs>
                     </cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1"/>
                     <cell role="data" rows="1" cols="1">
                        <rs type="person" ref="pmb2121">Schnitzler</rs>
                     </cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1"/>
                     <cell role="data" rows="1" cols="1">Redactionstrinkgelder</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1"/>
                     <cell role="data" rows="1" cols="1">Briefpapier</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1"/>
                     <cell role="data" rows="1" cols="1">Leiter neben <rs type="place" ref="pmb337">Hotel de France</rs> (2 Meter 70, da Kasten 2 M 30 ist)</cell>
                  </row>
               </table>
            </p>
            <p>
               <table cols="3" rows="16">
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1">Einnahmen</cell>
                     <cell role="data" rows="1" cols="1">Von oder für</cell>
                     <cell role="data" rows="1" cols="1">
                        <anchor type="textConst" xml:id="TD450-1">Ausgaben</anchor>
                     </cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1"/>
                     <cell role="data" rows="1" cols="1">Nach Bezalg von 500 Kr an <rs type="person" ref="pmb13682">Rosa</rs> bleiben mir</cell>
                     <cell role="data" rows="1" cols="1"/>
                  </row>
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1">600 Kr.</cell>
                     <cell role="data" rows="1" cols="1"/>
                     <cell role="data" rows="1" cols="1"/>
                  </row>
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1"/>
                     <cell role="data" rows="1" cols="1">
                        <rs type="person" ref="pmb27721">Brieftr</rs>
                     </cell>
                     <cell role="data" rows="1" cols="1">10 Kr</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1"/>
                     <cell role="data" rows="1" cols="1">
                        <rs type="person" ref="pmb27722">Gemeindediener</rs>
                     </cell>
                     <cell role="data" rows="1" cols="1">2 Kr</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1"/>
                     <cell role="data" rows="1" cols="1">für Hefte</cell>
                     <cell role="data" rows="1" cols="1">4 Kr</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1"/>
                     <cell role="data" rows="1" cols="1">Commission</cell>
                     <cell role="data" rows="1" cols="1">1 Kr</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1"/>
                     <cell role="data" rows="1" cols="1">Apotheke</cell>
                     <cell role="data" rows="1" cols="1">2 68 h</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1"/>
                     <cell role="data" rows="1" cols="1">Trinkgelder für Diener des <rs type="org" ref="pmb29414">N. W.</rs> u. der <rs type="org" ref="pmb29421">Ö. V.</rs>
                     </cell>
                     <cell role="data" rows="1" cols="1">30 Kr.</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1"/>
                     <cell role="data" rows="1" cols="1">
                        <rs type="person" ref="pmb28184">Ferdinand</rs> Comission</cell>
                     <cell role="data" rows="1" cols="1">2</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1"/>
                     <cell role="data" rows="1" cols="1">Wagen</cell>
                     <cell role="data" rows="1" cols="1">3 30</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1"/>
                     <cell role="data" rows="1" cols="1">
                        <rs type="person" ref="pmb27721">Briefträger</rs>
                     </cell>
                     <cell role="data" rows="1" cols="1">3</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1"/>
                     <cell role="data" rows="1" cols="1">Feuerwehrball</cell>
                     <cell role="data" rows="1" cols="1">4</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1">39 Kr</cell>
                     <cell role="data" rows="1" cols="1">vom <rs type="org" ref="pmb29655">Rubinverlag</rs> am 4/1 erhalten</cell>
                     <cell role="data" rows="1" cols="1"/>
                  </row>
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1"/>
                     <cell role="data" rows="1" cols="1"/>
                     <cell role="data" rows="1" cols="1">
                        <hi rend="overline">61 98</hi>
                     </cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                     <cell role="data" rows="1" cols="1"/>
                     <cell role="data" rows="1" cols="1">An <rs type="person" ref="pmb28076">Ludwig</rs> noch am 31. Dez. bezalt</cell>
                     <cell role="data" rows="1" cols="1">30 Kr</cell>
                  </row>
               </table>
            </p>
         </div>
      </body>
   </text>
